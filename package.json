{
  "name": "acc-exporter",
  "version": "1.0.0",
  "description": "ACC Metadata Exporter",
  "main": "dist/main.js",
  "homepage": "./",
  "scripts": {
    "dev": "npm run build:main && concurrently \"npm run dev:react\" \"npm run dev:electron\"",
    "dev:react": "webpack serve --mode development --config-name renderer",
    "dev:electron": "wait-on http://localhost:3000 && electron .",
    "build:main": "webpack --mode development --config-name main && webpack --mode development --config-name preload",
    "build": "webpack --mode production",
    "start": "electron dist/main.js",
    "pack": "electron-builder --dir",
    "dist": "npm run build && electron-builder"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.1",
    "@mui/material": "^5.14.1",
    "axios": "^1.5.0",
    "keytar": "^7.9.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.16.0"
  },
  "devDependencies": {
    "@types/node": "^20.5.9",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "concurrently": "^8.2.0",
    "css-loader": "^6.8.1",
    "electron": "^26.0.0",
    "electron-builder": "^24.6.4",
    "electron-is-dev": "^3.0.0",
    "html-webpack-plugin": "^5.5.3",
    "style-loader": "^3.3.3",
    "ts-loader": "^9.5.0",
    "typescript": "^5.2.2",
    "wait-on": "^7.0.1",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  }
}